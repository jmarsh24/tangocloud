{
  "name": "app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "API_URL=http://localhost:3000/graphql expo start",
    "start:staging": "API_URL=https://tangocloud.app/graphql expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "graphql:schema": "rover graph introspect http://localhost:3000/graphql --output shared/schema.gql",
    "graphql:types": "graphql-codegen",
    "format": "prettier --write \"./**/*.{tsx,ts,css,scss,json,yml}\"",
    "lint": "yarn lint:types && yarn lint:style && yarn lint:format",
    "lint:types": "tsc --noEmit",
    "lint:style": "eslint \"**/*.{tsx,ts}\" --max-warnings 0",
    "lint:format": "prettier --list-different \"**/*.{css,json}\"",
    "release": "rm .gitignore && eas build -p ios --auto-submit --non-interactive --profile production",
    "release:alpha": "rm .gitignore && eas build -p ios --auto-submit --non-interactive --profile alpha"
  },
  "dependencies": {
    "react-navigation": "^5.0.0",
    "@apollo/client": "^3.8.1",
    "@formatjs/intl-getcanonicallocales": "^2.2.1",
    "@formatjs/intl-locale": "^3.3.2",
    "@formatjs/intl-pluralrules": "^5.2.4",
    "@formatjs/intl-relativetimeformat": "^11.2.4",
    "@nerdgeschoss/react-use-form-library": "^0.0.64",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/elements": "^1.3.18",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@react-navigation/stack": "^6.3.17",
    "@reduxjs/toolkit": "^1.9.5",
    "@shopify/react-native-skia": "0.1.196",
    "expo": "~50.0.3",
    "expo-auth-session": "~5.0.2",
    "expo-constants": "^14.4.2",
    "expo-dev-client": "~2.4.8",
    "expo-linear-gradient": "~12.3.0",
    "expo-linking": "^5.0.2",
    "expo-localization": "~14.3.0",
    "expo-random": "^13.2.0",
    "expo-router": "2.0.0",
    "expo-secure-store": "~12.3.1",
    "expo-status-bar": "^1.6.0",
    "expo-updates": "~0.18.12",
    "expo-web-browser": "~12.3.2",
    "graphql": "^16.8.0",
    "i18n-js": "^4.3.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-keyed-flatten-children": "^2.2.1",
    "react-native": "^0.73.2",
    "react-native-collapsible": "^1.6.1",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-pager-view": "6.2.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-web": "^0.19.7",
    "redux": "^4.2.1",
    "redux-database": "^0.0.20"
  },
  "devDependencies": {
    "@apollo/rover": "^0.18.0",
    "@babel/core": "^7.22.10",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.4",
    "@types/lodash": "^4.14.197",
    "@types/react": "~18.2.45",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.0.2",
    "typescript": "~5.1.6"
  },
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "resolutions": {
    "@graphql-typed-document-node/core": "3.2.0"
  },
  "volta": {
    "node": "18.18.2",
    "yarn": "1.22.19"
  }
}
