{
  "name": "app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "lint": "yarn lint:style && yarn lint:types && yarn lint:format",
    "lint:style": "next lint -f github-annotations",
    "lint:types": "tsc --noEmit",
    "lint:format": "prettier --list-different \"src/**/*.{tsx,ts,css,scss,json,yml}\" \"*.yml\"",
    "i18n:pull": "translate pull --format i18n-js --out src/locale/en.json --locale en",
    "i18n:push": "translate push --format i18n-js --in src/locale/en.json --locale en",
    "format": "prettier --write \"src/**/*.{tsx,ts,css,scss,json,yml}\" \"*.yml\"",
    "test": "playwright test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o public/storybook --test",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo export --platform web"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.1",
    "@expo/vector-icons": "^14.0.0",
    "@nerdgeschoss/react-use-form-library": "^0.0.68",
    "@nerdgeschoss/shimmer": "^0.0.34",
    "@react-navigation/native": "^6.0.2",
    "classnames": "^2.5.1",
    "date-fns": "^3.0.6",
    "expo": "~50.0.4",
    "expo-av": "~13.10.4",
    "expo-font": "~11.10.2",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.6",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "expo-web-browser": "~12.8.2",
    "i18n-js": "^4.3.2",
    "luxon": "^3.4.4",
    "react": "18.2.0",
    "react-day-picker": "^8.10.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "^14.1.0",
    "react-native-web": "~0.19.6",
    "react-popper": "^2.3.0",
    "react-select": "^5.8.0",
    "recharts": "^2.10.4",
    "sass": "^1.69.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@playwright/test": "^1.41.1",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^7.6.7",
    "@storybook/nextjs": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/testing-library": "^0.2.2",
    "@types/luxon": "^3.3.8",
    "@types/node": "^20.11.8",
    "@types/react": "~18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-formatter-github-annotations": "^0.1.0",
    "eslint-plugin-storybook": "^0.6.15",
    "prettier": "^3.1.1",
    "storybook": "^7.6.7",
    "typescript": "^5.3.3"
  },
  "private": true,
  "volta": {
    "node": "20.11.0",
    "yarn": "3.6.3"
  }
}
